<div class="container">
  <form class="form-signin" action="/sessions" method="post" >
      <input id="auxiliar" type="hidden" value="0"/>
        <h4 class="form-signin-heading text-center" style="margin-bottom: 20px;">Datos de Usuario</h4>
        <label for="nombre" class="control-label">Nombre</label>
        <input type="text" id="nombre" class="form-control" placeholder="Nombre" required autofocus style="margin-bottom: 15px;">
        <label for="apellido" class="control-label">Apellido</label>
        <input type="text" id="apellido" class="form-control" placeholder="Apellido" required autofocus style="margin-bottom: 15px;">
        <label for="usuario" class="control-label">Nombre de usuario</label>
        <input type="text" id="usuario" class="form-control" placeholder="Nombre de usuario" required autofocus style="margin-bottom: 15px;">
        <label for="email" class="control-label">Direccion de correo</label>
        <input type="email" id="email" class="form-control" placeholder="Direccion de correo" required autofocus style="margin-bottom: 15px;">
        <label for="password" class="control-label">Contrasena</label>
        <input type="password" id="password" class="form-control" placeholder="Contrasena" required>
        <label for="password2" class="control-label">Repita la contrasena</label>
        <input type="password" id="password2" class="form-control" placeholder="Repita la contrasena" required>
        <div class="form-group">
            <label class="control-label" for="facultad">Seleccione Facultad</label>
            <select id="facultad" class="form-control">
                <option>1</option>
                <option>2</option>
                <option>3</option>
            </select>
        </div>
        <button class="btn btn-lg btn-primary btn-block" type="submit" style="margin-top: 20px;">Crear Cuenta</button>
    </form>
</div>
